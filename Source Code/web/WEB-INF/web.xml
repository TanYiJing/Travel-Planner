<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>userLocProc</servlet-name>
        <servlet-class>Servlet.userLocProc</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>userPathProc</servlet-name>
        <servlet-class>Servlet.userPathProc</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteServlet</servlet-name>
        <servlet-class>Servlet.DeleteServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DisplayServlet</servlet-name>
        <servlet-class>Servlet.DisplayServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LocationMaintenance</servlet-name>
        <servlet-class>Servlet.LocationMaintenance</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModifyServlet</servlet-name>
        <servlet-class>Servlet.ModifyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>userLocProc</servlet-name>
        <url-pattern>/userLocProc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>userPathProc</servlet-name>
        <url-pattern>/userPathProc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteServlet</servlet-name>
        <url-pattern>/DeleteServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DisplayServlet</servlet-name>
        <url-pattern>/DisplayServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LocationMaintenance</servlet-name>
        <url-pattern>/LocationMaintenance</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModifyServlet</servlet-name>
        <url-pattern>/ModifyServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
